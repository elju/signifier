{
  "name": "Signifier",
  "version": "0.1007",
  "manifest_version": 2,
  "description": "Yo dawg, I heard you like the internet so we...",
  "browser_action": {
    "default_icon": "images/icon2.png",
	"default_popup": "chrome/popup.html"
  },
  "background": {
    "scripts": ["chrome/main.js"]
  },
  "icons":{
    "128":"images/icon.png"
  },
  "content_scripts": [ {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scripts/socket.io.js", "scripts/underscore.js", "scripts/jquery.min.js", "content-script/signifier.js"],
	  "css": ["css/mainstyle.css"]
  }  ],
  "permissions": [
  	"tabs", "http://127.0.0.1:7000/*", "https://127.0.0.1:7000/*"
  ]
}
